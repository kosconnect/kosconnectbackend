{
    "version": 2,
    "builds": [
      {
        "src": "main.go",
        "use": "@vercel/go"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.go"
      }
    ],
    "rewrites": [
      { "source": "/auth/assign-role", "destination": "/auth/assign-role.html" }
    ]
  }
  